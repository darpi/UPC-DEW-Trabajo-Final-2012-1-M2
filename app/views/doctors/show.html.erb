<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
        <link rel="stylesheet" href="../assets/ie.css" type="text/css">
        <link rel="stylesheet" href="../assets/print.css" type="text/css">
        <link rel="stylesheet" href="../assets/screen.css" type="text/css">
        <link rel="stylesheet" href="../assets/styles.css" type="text/css">
		<script src="../assets/SpryTabbedPanels.js" type="text/javascript"></script>
		<link href=../"assets/SpryTabbedPanels.css" rel="stylesheet" type="text/css">
        <script language="JavaScript" src="../assets/scripts.js"></script>
        <title>Veterinaria Patita</title>
    </head>
    <body>
        <div class="container-page">

            <!-- Inicio Cabecera del Sistema -->
            <div id="encabezado" class="span-24 last">				
                <div id="logo" class="span-24 last">
                    <img src="../assets/logo.png" />
                </div>
                <!--div id="aplicacion" class="span-7 last">
                    Nombre de la Aplicaci&oacute;n
                </div-->
                				
            </div>
            <!-- Fin Cabecera del Sistema -->

			<!-- Inicio Menu del Sistema -->
				<div class="span-24 last">
					<div id="TabbedPanels1" class="TabbedPanels">
					  <ul class="TabbedPanelsTabGroup">
						<li class="TabbedPanelsTab" tabindex="0">
							<a href="#">Citas de Vacunas y Tareas</a>
						</li>
					  <li class="TabbedPanelsTab" tabindex="0"><a href="#" onclick="javascript:pacientes();">Pacientes</a></li>
						<li class="TabbedPanelsTab"  tabindex="0"><a href="#" onclick="javascript:clientes();">Clientes</a></li>
						<li class="TabbedPanelsTab" tabindex="0"><a href="#" onclick="javascript:prospectos();">Prospectos</a></li>
						<li class="TabbedPanelsTab" tabindex="0"><a href="#" onclick="javascript:doctores();">Doctores</a></li>
									  </ul>
					  <div class="TabbedPanelsContentGroup">
						&nbsp;
						<div class="TabbedPanelsContent" style="display: none; ">
						&nbsp;
						</div>
						<div class="TabbedPanelsContent" style="display: none; ">
							&nbsp;
						</div>
						<div class="TabbedPanelsContent" style="display: none; ">
							&nbsp;
						</div>
						<div class="TabbedPanelsContent" style="display: none; ">
							&nbsp;
						</div>
					  </div>
					</div>
				</div>
				
				<div class="span-24 last">
					<ul class="menu-3">
						<li><%= link_to 'Nuevo Doctor', new_doctor_path %></li> 
						
					</ul>
				</div>
            <!-- Fin Menu del Sistema -->
			
            <!-- Inicio Cuerpo de la Pagina -->
       
				<p id="notice"><%= notice %></p>

				<p>
				  <b>Nombre:</b>
				  <%= @doctor.administrator %>
				</p>

				<p>
				  <b>Apellido:</b>
				  <%= @doctor.name_surname %>
				</p>

				<p>
				  <b>Email:</b>
				  <%= @doctor.mail %>
				</p>

				<p>
				  <b>Telefono:</b>
				  <%= @doctor.phone %>
				</p>

				<p>
				  <b>Usuario:</b>
				  <%= @doctor.user %>
				</p>


				<%= link_to 'Edicion', edit_doctor_path(@doctor) %> |
				<%= link_to 'Regresar', doctors_path %>

			<script type="text/javascript">
			var TabbedPanels1 = new Spry.Widget.TabbedPanels("TabbedPanels1");
			
			  </script>
            <!-- Fin Cuerpo de la Pagina -->

            <!-- Inicio Pie del Sistema --> 
            <div id="pie" class="span-24 last">
                Derechos Reservados
            </div>
            <!-- Fin Pie del Sistema --> 
        </div>
    </body>
</html>
