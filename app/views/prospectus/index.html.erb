					<ul class="menu-3">
						<li><%= link_to 'Nuevo Prospecto', new_prospectu_path %></li> 
						
					</ul>
				</div>
            <!-- Fin Menu del Sistema -->
			
            <!-- Inicio Cuerpo de la Pagina -->
            <div class="span-24 last">
				<h3>Listado de Prospectos</h3>
			</div>
					
			<div id="formulario" class="span-24 last">				
				<table>
				  <tr>
					<th>Nombre</th>
					<th>Email</th>
					<th>Direccion</th>
					<th>Image</th>
					<th>Telefono Casa</th>
					<th>Numero Celular</th>
					<th></th>
					<th></th>
					<th></th>
				  </tr>

				<% @prospectus.each do |prospectu| %>
				  <tr>
					<td><%= prospectu.name %></td>
					<td><%= prospectu.email %></td>
					<td><%= prospectu.address %></td>
					<td><%= prospectu.image %></td>
					<td><%= prospectu.phone %></td>
					<td><%= prospectu.cell_phone %></td>
					<td><%= link_to 'Ver Detalle', prospectu %></td>
					<td><%= link_to 'Editar', edit_prospectu_path(prospectu) %></td>
					<td><%= link_to 'Eliminar', prospectu, confirm: 'Esta seguro de eliminar el prospecto?', method: :delete %></td>
					<td><%= link_to 'Cambiar a DueÃ±o', prospectu, confirm: 'Esta seguro de cambiar a Dueno?' %></td>
				  </tr>
				<% end %>
		
				</table>

